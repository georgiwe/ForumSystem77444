@model string
<div class="row form-group">
    <div class="col-md-2">
        @Html.LabelFor(m => m, new { @class = "form-label" })
    </div>
    <div class="col-md-5">
        @Html.TextBoxFor(m => m, new { @class = "form-control" })
    </div>
    <div>
        @Html.ValidationMessageFor(m => m)
    </div>
</div>
